MAXN = 10**6 + 7
isp = [True] * MAXN 

def snt():
    isp[0] = isp[1] = False 
    for i in range(2, int(MAXN**0.5) + 1):
        if(isp[i] == True):
            for j in range(i*i, MAXN, i):
                isp[j] = False 
snt()
n = int(input())
cnt = 0
        
for i in range(2, n):
    if(i*i > n): break
    if(isp[i] == True): cnt += 1
print(cnt)
