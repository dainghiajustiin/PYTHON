def init():

    return

def solve():
    init()

    n = int(input())
    S = 0

    if(n <= 0): print("NO")
    else:
        for i in range(1, int(n**0.5) + 1, 1):
            if(n % i == 0):
                S += i
                if(i*i != n): S += n//i
        
        if(2*n == S): print("YES")
        else: print("NO")

    return

solve()
