def init():

    return

def get(N):
    i = 5
    res = 0
    while(i <= N):
        res += N//i
        i *= 5 
    return res 

def solve():
    init()

    T = int(input())

    for _ in range(T):
        N = int(input())
        L = 0
        R = 5*N
        ans = 0
        while(L <= R):
            M = (L + R) >> 1

            if(get(M) >= N):
                ans = M 
                R = M - 1
            else:
                L = M + 1 
        print(ans)
    return

solve()
