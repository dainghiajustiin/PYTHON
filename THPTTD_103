TASK = "Parity"
FILE_INPUT = TASK + ".inp"
FILE_OUTPUT = TASK + ".OUT"

with open(FILE_INPUT, 'r') as fileIn, open(FILE_OUTPUT, 'w') as fileOut:
    T = int(fileIn.readline().strip())
    for i in range(T):
        N = int(fileIn.readline().strip())
        cnt = 0
        for j in range(63, -1, -1):
            if(N >> j & 1): cnt += 1
        
        if(cnt&1): fileOut.write("odd\n")
        else: fileOut.write("even\n")
